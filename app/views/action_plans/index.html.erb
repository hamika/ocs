<h1>Action Plans</h1>

<table>
  <thead>
    <tr>
      <th>Term from</th>
      <th>Term to</th>
      <th>Target</th>
      <th>Achievement</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @action_plans.each do |action_plan| %>
      <tr>
        <td><%= action_plan.term_from %></td>
        <td><%= action_plan.term_to %></td>
        <td><%= action_plan.target %></td>
        <td><%= action_plan.achievement %></td>
        <td><%= action_plan.body %></td>
        <td><%= link_to 'Show', action_plan %></td>
        <td><%= link_to 'Edit', edit_action_plan_path(action_plan) %></td>
        <td><%= link_to 'Destroy', action_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Dashboards', dashboards_path(action_plan_id: action_plan.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Action Plan', new_action_plan_path %>
